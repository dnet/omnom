{
	"name": "Tagr",
	"version": "1.0",
	"description": "A bookmarking extension.",
	"content_scripts": [ {
		"js": [ "jquery-1.3.2.min.js", "tagr.js" ],
		"matches": [ "http://*/*", "https://*/*", "ftp://*/*" ]
	} ],
	"background_page": "background.html",
	"browser_action": {
		"default_icon": "icon.png",
		"popup": "popup.html"
	},
	"options_page": "options.html",
	"permissions": [
		"bookmarks", "tabs"
	]
}
